{
  "name": "taskml",
  "version": "0.1.0-alpha.2",
  "description": "TaskML parser and renderer - Core TypeScript library",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "browser": "./dist/index.browser.js",
  "unpkg": "./dist/index.browser.js",
  "jsdelivr": "./dist/index.browser.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/index.browser.js",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "taskml",
    "parser",
    "ai-agents",
    "task-management",
    "visualization"
  ],
  "author": "Yuval Suede",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yuvalsuede/taskml.git",
    "directory": "packages/core"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
